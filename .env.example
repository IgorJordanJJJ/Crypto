# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DATABASE=crypto_analytics

# API Keys  
# Note: Bybit API is free for public data and doesn't require an API key
# Note: DefiLlama API is free and doesn't require an API key

# Application Settings
DEBUG=false
APP_NAME="Crypto DeFi Analyzer"
APP_VERSION=0.1.0

# Batch Processing Settings
BATCH_SIZE=100
FETCH_INTERVAL_MINUTES=15
MAX_RETRIES=3
REQUEST_TIMEOUT=30

# API URLs (usually don't need to change)
BYBIT_API_URL=https://api.bybit.com/v5
DEFILLAMA_API_URL=https://api.llama.fi

# Superset Configuration
SUPERSET_SECRET_KEY=your_secret_key_here
SUPERSET_DB_PASSWORD=supersetpassword

# Docker-specific settings (used in docker-compose.yml)
# Leave empty for default behavior
POSTGRES_PASSWORD=password
COINGECKO_API_KEY=
DEBUG=false
DEV_MODE=false

# Production Server Configuration (Docker)
WORKERS=2
THREADS=4
TIMEOUT=60